{
    "env": {
        "es6": true,
        "node": true,
        "browser": true
    },
    "globals": {
    },
    "rules": {
        // http://eslint.org/docs/rules/
        "strict": 0,
        "eqeqeq": 2,
        "quotes": [2, "single", "avoid-escape"],
        "no-undef": 2,
        "no-mixed-spaces-and-tabs": 2,
        "new-cap": 0,
        "no-caller": 1,
        "no-unused-vars": 2,
        "no-trailing-spaces": 2,
        "no-shadow": 2,
        "eol-last": 1,
        "key-spacing": 0,
        "comma-dangle": 0,
        "curly": 0,
        "camelcase": 0,
        "no-underscore-dangle": 0
    }
}
